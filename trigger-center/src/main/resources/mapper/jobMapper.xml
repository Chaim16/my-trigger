<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.onedawn.mytrigger.triggercenter.dao.mapper.JobMapper">
    <insert id="register" parameterType="cn.onedawn.mytrigger.pojo.Job">
        insert into 任务 values(
            null ,
            #{status},
            #{triggerTime},
            #{remove},
            #{callName},
            #{callData},
            #{callType},
            #{callHost},
            #{cron},
            #{createTime},
            #{modifyTime},
            #{app}
        )
    </insert>

</mapper>
